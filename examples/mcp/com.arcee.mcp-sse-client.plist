<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.arcee.mcp-sse-client</string>
    <key>ProgramArguments</key>
    <array>
        <string>/caminho/para/seu/venv/bin/python</string>
        <string>/caminho/para/seu/projeto/arcee_cli/examples/mcp/mcp_sse_app.py</string>
